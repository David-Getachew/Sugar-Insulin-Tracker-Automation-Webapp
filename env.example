# env.example - copy to .env and fill values before running
# Vite frontend exposed vars (safe to expose anon key)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# N8N webhook for emergency events
N8N_EMERGENCY_WEBHOOK_URL=https://your-n8n-webhook.example.com/path

# Demo account (only for local QA). Demo accounts are read-only.
DEMO_USER_EMAIL=demo@example.com
DEMO_USER_PASSWORD=DemoPass123

# Do NOT commit service role or other server-only secrets
# SUPABASE_SERVICE_ROLE_KEY must remain server-side only and never in client repo